{
    "builds": [
        {
            "src": "frontend/package.json",
            "use": "@vercel/next"
        },
        {
            "src": "api/index.py",
            "use": "@vercel/python"
        }
    ],
    "rewrites": [
        {
            "source": "/graphql",
            "destination": "/api/index.py"
        }
    ],
    "routes": [
        {
            "src": "/graphql",
            "dest": "/api/index.py"
        },
        {
            "src": "/(.*)",
            "dest": "/frontend/$1"
        }
    ]
}